    "import numpy as np\n",
    "import cv2\n",
    "img = cv2.imread('download.jpeg', -1)\n",
    "cv2.imshow('image unchanged', img)\n",
    "k = cv2.waitKey(0) & 0xFF\n",
    "if k == 27:\n",
    "    cv2.destroyAllWindows()\n",
    "elif k == 115:\n",
    "    cv2.imwrite('unchangedimage.jpg', img)\n",
    "    cv2.destroyAllWindows()"
   ]
  }
